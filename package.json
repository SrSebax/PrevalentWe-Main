  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next",
    "generate_build": "cosmo && npx prisma generate && prettier --config .prettierrc prisma/generated/graphql/**/*.ts --write && next build",
    "build": "next build",
    "start": "next start",
    "lint": "eslint components/** utils/** pages/** hooks/**",
    "lint:fix": "eslint --fix components/** utils/** pages/** hooks/**",
    "prepare": "husky install",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "createPage": "node ./createPage.ts",
    "aurora": "aurora && npx prisma format",
    "cosmo": "cosmo && prettier --config .prettierrc prisma/generated/graphql/**/*.ts --write",
    "generate": "yarn aurora && npx prisma generate && yarn cosmo",
    "migrate-dev": "yarn aurora && prisma migrate dev",
    "migrate-deploy": "yarn aurora && npx prisma migrate deploy"
  },
